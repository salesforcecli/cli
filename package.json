{
  "name": "@salesforce/cli",
  "description": "The Salesforce CLI",
  "version": "0.0.4",
  "author": "Salesforce",
  "bin": {
    "sf": "./bin/run"
  },
  "homepage": "https://github.com/salesforcecli/cli",
  "bugs": "https://github.com/salesforcecli/cli/issues",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "oclif": {
    "bin": "sf",
    "scope": "salesforce",
    "topicSeparator": " ",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@salesforce/plugin-env",
      "@salesforce/plugin-functions",
      "@salesforce/plugin-login",
      "@salesforce/plugin-project",
      "@salesforce/plugin-project-org"
    ],
    "devPlugins": [
      "@oclif/plugin-command-snapshot"
    ]
  },
  "dependencies": {
    "@oclif/core": "^0.5.20",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^2",
    "@salesforce/plugin-env": "0.0.7",
    "@salesforce/plugin-functions": "0.2.13",
    "@salesforce/plugin-login": "0.0.4",
    "@salesforce/plugin-project": "0.0.4",
    "@salesforce/plugin-project-org": "0.0.2",
    "tslib": "^2.3.0"
  },
  "pinnedDependencies": [
    "@salesforce/plugin-env",
    "@salesforce/plugin-functions",
    "@salesforce/plugin-login",
    "@salesforce/plugin-project",
    "@salesforce/plugin-project-org"
  ],
  "repository": "salesforcecli/cli",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "-clean all",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif-dev manifest && oclif-dev readme",
    "pretest": "sf-compile-test",
    "test": "echo disable # sf-test",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "commit-msg": "sf-husky-commit-msg",
      "pre-commit": "sf-husky-pre-commit",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-command-snapshot": "^2.1.2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^2.1.2",
    "@salesforce/dev-scripts": "^0.9.15",
    "@salesforce/prettier-config": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chai": "^4.3.4",
    "eslint": "^7.29.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "globby": "^11.0.4",
    "husky": "^4.2.5",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "oclif": "^1.18.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "sinon": "^11.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
